class StaticAndNonStatic 
{
	static String name="NIkita";
	int age=20;
	static String email;
	long contact;
	{
		System.out.println("NOn static block");
		  a();

		System.out.println(name);
		System.out.println(age);
			email="233423";
		contact=1233;
		

		
	}
	static
	{
		System.out.println("Static block");
