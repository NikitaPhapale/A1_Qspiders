class Pattern15
{
	public static void main(String[] args) 
	{
		int b=1;
		
		for(int i=1;i<=5;i++)
		{
			int a=7;
			int c=2;
			b=i;
			for(int j=1;j<=i;j++)
			{
				
				if(j%2!=0)
				{
					
					System.out.print(b+" ");
					b+=c;
					c+=2;
				}
				else
				{
					b+=a;
					System.out.print(b+" ");
					
					a-=2;
					
				}
			}
			System.out.println();
		}
		
	
	}
}
