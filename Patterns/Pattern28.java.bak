class Pattern28 
{
	public static void main(String[] args) 
	{
		
		//int sum=1;
		for(int i=1;i<5;i++)
		{
			int sum=i;
			int a=7;
			int b=1;
			for(int j=1;j<5;j++)
			{	
				if(j%2!=0)
				{
					
					System.out.print(sum+" ");
					sum+=b;
					b+=2;
				}
				
				else
				{
					System.out.print(sum+" ");
					sum+=a;
					a-=2;
					
				}
				

		}
		System.out.println();
		}
	}
}
